{
    "name": "keboola/mongodb-extractor",
    "license": "MIT",
    "type": "project",
    "description": "Keboola MongoDb Extractor",
    "keywords": ["keboola", "mongodb", "extractor"],
    "authors": [
        {
            "name": "Vladimír Kriška",
            "email": "vlado@keboola.com"
        }
    ],
    "require": {
        "symfony/process": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/console": "^5.0",
        "nette/utils": "^2.5",
        "symfony/serializer": "^5.0",
        "keboola/ssh-tunnel": "^2.0",
        "monolog/monolog": "^2.0",
        "symfony/config": "^5.0",
        "keboola/csvmap": "^0.6.0",
        "php": "^7.4",
        "ext-json": "*",
        "keboola/csv": "^1.1",
        "league/uri": "^6.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.3",
        "keboola/coding-standard": ">=9.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Keboola\\": "src/Keboola/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\MongoDbExtractor\\Tests\\": "tests/Keboola/MongoDbExtractor/"
        }
    },
    "scripts": {
        "phpcs": "phpcs -n --ignore=vendor --extensions=php .",
        "phpcbf": "phpcbf -n --ignore=vendor --extensions=php .",
        "phpunit": "phpunit"
    }
}
